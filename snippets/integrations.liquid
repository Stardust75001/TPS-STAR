{%- comment -%} Ensure Sentry init includes autoSessionTracking + release {%- endcomment -%}
<script>
  (function(){
    var sentryDsn = {{ sentry_dsn | json }};
    if (!sentryDsn) return;
    var sentryRelease = "tps-star@{{ 'now' | date: '%Y-%m-%d' }}";

    // load Sentry SDK (if not already loaded)
    if (!window.Sentry) {
      var s = document.createElement('script');
      s.src = 'https://browser.sentry-cdn.com/8.36.0/bundle.tracing.replay.min.js';
      s.crossOrigin = 'anonymous';
      s.async = true;
      s.onload = initSentry;
      document.head.appendChild(s);
    } else {
      initSentry();
    }

    function initSentry(){
      try {
        Sentry.init({
          dsn: sentryDsn,
          release: sentryRelease,
          autoSessionTracking: true,
          integrations: [new Sentry.Integrations.BrowserTracing()],
          tracesSampleRate: 0.05
        });
      } catch(e){}
    }
  })();
</script>

